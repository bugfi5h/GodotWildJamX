; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Area2D",
"class": "Glorb",
"language": "GDScript",
"path": "res://items/glorbs/Glorb.gd"
} ]
_global_script_class_icons={
"Glorb": ""
}

[application]

config/name="WallsAreItchy"
run/main_scene="res://ui/MainMenue.tscn"
config/icon="res://assets/icon.png"

[autoload]

SceneLoader="*res://globals/SceneLoader.tscn"
Helper="*res://globals/Helper.gd"

[display]

window/size/width=1920
window/size/height=1080
window/stretch/mode="2d"
window/stretch/aspect="keep"

[layer_names]

2d_physics/layer_1="enviroment"
2d_physics/layer_2="player"
2d_physics/layer_3="enemy"
2d_physics/layer_4="item"

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
